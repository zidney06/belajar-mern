{
	// Visit https://aka.ms/tsconfig to read more about this file
	"compilerOptions": {
		"target": "ES2020",
		"module": "node16",
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"removeComments": true
	},
	"include": ["./src"],
	"exclude": ["node_modules", "uploads"]
}
